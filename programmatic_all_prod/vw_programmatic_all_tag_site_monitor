WITH A AS (
  WITH
    AdX_programmatic_all AS (
      SELECT
        Date,
        Source,
        Tag_Site,
        Ad_Size_initial,
        Device_type_initial,
        Country_initial,
        SUM(Net_Revenue) AS Net_Revenue,
        --MIN(Date) AS min_date,
        --MAX(Date) AS max_date
      FROM `meredith-switchboard.programmatic_all.vw_AdX_programmatic_all`
      GROUP BY 1,2,3,4,5,6

      UNION ALL

      SELECT
        Date,
        Source,
        Tag_Site,
        Ad_Size_initial,
        Device_type_initial,
        Country_initial,
        SUM(Net_Revenue) AS Net_Revenue,
        --MIN(Date) AS min_date,
        --MAX(Date) AS max_date
      FROM `meredith-switchboard.programmatic_all.vw_AdX_API_programmatic_all`
      GROUP BY 1,2,3,4,5,6

      UNION ALL

      SELECT
        Date,
        Source,
        Tag_Site,
        Ad_Size_initial,
        Device_type_initial,
        Country_initial,
        SUM(Net_Revenue) AS Net_Revenue,
        --MIN(Date) AS min_date,
        --MAX(Date) AS max_date
      FROM `meredith-switchboard.programmatic_all.vw_AdX_GAM_programmatic_all`
      GROUP BY 1,2,3,4,5,6

      UNION ALL

      SELECT
        Date,
        Source,
        Tag_Site,
        Ad_Size_initial,
        Device_type_initial,
        Country_initial,
        SUM(Net_Revenue) AS Net_Revenue,
        --MIN(Date) AS min_date,
        --MAX(Date) AS max_date
      FROM `meredith-switchboard.programmatic_all.vw_AdX_API_PG_and_PD_programmatic_all`
      GROUP BY 1,2,3,4,5,6
  ),

    nonAdX_programmatic_all AS (
      SELECT
        Date,
        Source,
        Tag_Site,
        Ad_Size_initial,
        Device_type_initial,
        Country_initial,
        SUM(Net_Revenue) AS Net_Revenue,
        --MIN(Date) AS min_date,
        --MAX(Date) AS max_date
      FROM `meredith-switchboard.programmatic_all.vw_programmatic_all_nonAdX_partner_aggregate`
      GROUP BY 1,2,3,4,5,6
  ),

  programmatic_all AS (
    SELECT *
    FROM AdX_programmatic_all

    UNION ALL

    SELECT *
    FROM nonAdX_programmatic_all
  )

  SELECT
    Date,
    Tag_Site,
    Source,
    Ad_Size_initial,
    Device_type_initial,
    Country_initial,
    SUM(Net_Revenue) AS Net_Revenue,
    --min_date,
    --max_date
  FROM programmatic_all
  GROUP BY 1,2,3,4,5,6
),

B AS (
  SELECT
    Tag_Site as map_tag
  FROM `about-dss-01.dotdash_meredith.sheet_programmatic_tag_map`
  GROUP BY 1
)

SELECT
  Tag_Site,
  Date,
  Source,
  Ad_Size_initial,
  Device_type_initial,
  Country_initial,
  B.map_tag,
  SUM(Net_Revenue) AS Net_Revenue,
  --min_date,
  --max_date,
FROM A

LEFT JOIN B
ON A.Tag_Site = B.map_tag

WHERE
  B.map_tag IS NULL
  AND A.Tag_Site IS NOT NULL
  AND date >= DATE_SUB(date, INTERVAL 120 day)
  --AND Net_Revenue > 1

GROUP BY 1,2,3,4,5,6,7

--ORDER BY Net_Revenue DESC
