    Tag_Site,
    Ad_Size_initial,
    Device_type_initial,
    Country_initial
  ),

  Pacific_prog_all AS (
  SELECT
    A.Date,
    A.Source,
    A.Tag_Site,
    A.Ad_Size_initial,
    A.Device_type_initial,
    A.Country_initial,
    SUM(A.Net_Revenue) AS Net_Revenue,
    --MIN(A.Date) AS min_date,
    --MAX(A.Date) AS max_date
  FROM `meredith-switchboard.programmatic_all_partners.vw_programmatic_all_pacific`  AS A
  GROUP BY
    Date,
    Source,
    Tag_Site,
    Ad_Size_initial,
    Device_type_initial,
    Country_initial
),

  programmatic_all AS(
  SELECT *
  FROM AdX_programmatic_all_untagged

  UNION ALL

  SELECT *
  FROM programmatic_all_untagged

  UNION ALL

  SELECT *
  FROM Pacific_prog_all )

SELECT
    Date,
    Tag_Site,
    Source,
    Ad_Size_initial,
    Device_type_initial,
    Country_initial,
    SUM(Net_Revenue) AS Net_Revenue,
    --min_date,
    --max_date
FROM programmatic_all
GROUP BY
    Date,
    Source,
    Tag_Site,
    Ad_Size_initial,
    Device_type_initial,
    Country_initial
    --min_date,
    --max_date
),

B AS (
SELECT
    Tag_Site as map_tag
FROM `meredith-switchboard.summary_reports.programmatic_tag_mapping`
GROUP BY
    map_tag
)

SELECT
    Tag_Site,
    Date,
    Source,
    Ad_Size_initial,
    Device_type_initial,
    Country_initial,
    SUM(Net_Revenue) AS Net_Revenue,
    --min_date,
    --max_date,
    B.map_tag
FROM A
LEFT JOIN B
ON A.Tag_Site = B.map_tag
WHERE
    B.map_tag IS NULL
    AND A.Tag_Site IS NOT NULL
    AND date >= DATE_SUB(date, INTERVAL 120 day)
    --AND Net_Revenue > 1
GROUP BY
    A.Tag_Site,
    Date,
    Source,
    Ad_Size_initial,
    Device_type_initial,
    Country_initial,
    --min_date,
    --max_date,
    B.map_tag
--ORDER BY Net_Revenue DESC
