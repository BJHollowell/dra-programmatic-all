WITH

programmatic_all_untagged AS(
    SELECT * FROM `meredith-switchboard.zzz_bj_progall_test.vw_programmatic_all_index`

    UNION ALL

    SELECT * FROM `meredith-switchboard.zzz_bj_progall_test.vw_programmatic_all_trustx`

    UNION ALL

    SELECT * FROM `meredith-switchboard.zzz_bj_progall_test.vw_programmatic_all_teads`

    UNION ALL

    SELECT * FROM `meredith-switchboard.zzz_bj_progall_test.vw_programmatic_all_amazon_open`

    UNION ALL

    SELECT * FROM `meredith-switchboard.zzz_bj_progall_test.vw_programmatic_all_amazon_pmp`

    UNION ALL

    SELECT * FROM `meredith-switchboard.zzz_bj_progall_test.temp_rubicon`

    UNION ALL

    SELECT * FROM `meredith-switchboard.zzz_bj_progall_test.vw_programmatic_all_roundel`

    UNION ALL

    SELECT * FROM `meredith-switchboard.zzz_bj_progall_test.vw_programmatic_all_openx`

    UNION ALL

    SELECT * FROM  `meredith-switchboard.zzz_bj_progall_test.vw_programmatic_all_pubmatic`

    UNION ALL

    SELECT * FROM `meredith-switchboard.zzz_bj_progall_test.vw_programmatic_all_spotx`

    UNION ALL

    SELECT * FROM `meredith-switchboard.zzz_bj_progall_test.vw_programmatic_all_appnexus`

    UNION ALL

    SELECT * FROM `meredith-switchboard.zzz_bj_progall_test.vw_programmatic_all_medianet`

    UNION ALL

    SELECT * FROM  `meredith-switchboard.zzz_bj_progall_test.vw_programmatic_all_rogers`

    UNION ALL

    SELECT * FROM `meredith-switchboard.zzz_bj_progall_test.vw_programmatic_all_liveintent`

    UNION ALL

    SELECT * FROM  `meredith-switchboard.zzz_bj_progall_test.vw_programmatic_all_nativo`

    UNION ALL

    SELECT * FROM  `meredith-switchboard.zzz_bj_progall_test.vw_programmatic_all_adsense`

    UNION ALL

    SELECT * FROM `meredith-switchboard.zzz_bj_progall_test.vw_programmatic_all_solve`

    UNION ALL

    SELECT * FROM `meredith-switchboard.zzz_bj_progall_test.vw_programmatic_all_kargo`

    UNION ALL

    SELECT * FROM  `meredith-switchboard.zzz_bj_progall_test.vw_programmatic_all_gumgum`

    UNION ALL

    SELECT * FROM `meredith-switchboard.zzz_bj_progall_test.vw_programmatic_all_outbrain`

    UNION ALL

    SELECT * FROM `meredith-switchboard.zzz_bj_progall_test.vw_programmatic_all_videointelligence`

    UNION ALL

    SELECT * FROM `meredith-switchboard.zzz_bj_progall_test.vw_programmatic_all_youtube`

    UNION ALL

    SELECT * FROM `meredith-switchboard.zzz_bj_progall_test.vw_programmatic_all_yieldmo`

    UNION ALL

    SELECT * FROM `meredith-switchboard.zzz_bj_progall_test.vw_programmatic_all_triplelift`

    UNION ALL

    SELECT * FROM `meredith-switchboard.zzz_bj_progall_test.vw_programmatic_all_jeeng`

    UNION ALL

    SELECT * FROM `meredith-switchboard.zzz_bj_progall_test.vw_programmatic_all_aax`

    UNION ALL

    SELECT * FROM `meredith-switchboard.zzz_bj_progall_test.vw_programmatic_all_yahoo`

    UNION ALL

    SELECT * FROM `meredith-switchboard.zzz_bj_progall_test.vw_programmatic_all_pacific`
